record(bo, "$(SIG1)")
{
  field(DESC, "$(SIG1.DESC)")
  field(DTYP, "Bx9000_KL2408")
  field(OUT,  "@$(COUPLER):$(SLOT):W_DIG_OUT_1")
  field(ZNAM, "$(SIG1.ZNAM)")
  field(ONAM, "$(SIG1.ONAM)")
  field(PINI, "$(SIG1.PINI)")
}

