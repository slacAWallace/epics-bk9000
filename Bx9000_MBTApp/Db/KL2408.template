record(fanout, "$(SIG1)ReProc")
{
  field(DESC, "FANOUT Reproc")
  field(SCAN, "Event")
  field(EVNT, "$(REPROCEVT)")
  field(SELM, "All")
  field(LNK1, "$(SIG1)")
  field(LNK2, "$(SIG2)")
  field(LNK3, "$(SIG3)")
  field(LNK4, "$(SIG4)")
  field(PINI, "NO")
}

